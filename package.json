    "latest": "^0.2.0",
    "npm-watch": "^0.13.0",
    "projetoflexileaseautos": "file:",
    "sqlite3": "^5.1.7",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20"
  }
