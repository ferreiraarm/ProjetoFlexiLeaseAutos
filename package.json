{
    "name": "Projeto Flexilease Autos",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "typeorm": "typeorm-ts-node-commonjs",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts",
        "build": " npx tsc",
        "start": "nodemon --exec npx ts-node src/index.ts",
        "style:check": "prettier --check \"src/**/*.ts\" ",
        "style:fix": "prettier --write \"src/**/*.ts\" ",
        "typeorm:migrate:run": "typeorm-ts-node-commonjs --dataSource src/database/data-source-cli.ts",
        "typeorm:migrate": "yarn typeorm migration:run",
        "typeorm:generate": "yarn typeorm migration:generate src/database/migrations/Migration --timestamp",
        "typeorm:revert": "yarn typeorm migration:revert"
    },
